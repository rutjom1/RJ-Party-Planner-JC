{
    "entities": {
        "PartyEvent": {
            "title": "Party Event",
            "description": "Represents a party or event planned by a user.",
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "time": {
                    "type": "string"
                },
                "location": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "theme": {
                    "type": "string"
                },
                "imageUrl": {
                    "type": "string",
                    "format": "uri"
                },
                "imageHint": {
                    "type": "string"
                }
            },
            "required": [
                "name",
                "date",
                "time",
                "location",
                "description",
                "imageUrl",
                "imageHint"
            ]
        }
    },
    "auth": {
        "providers": []
    },
    "firestore": {
        "/events/{eventId}": {
            "schema": {
                "$ref": "#/entities/PartyEvent"
            },
            "description": "Stores all party events."
        }
    }
}
